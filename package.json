{
  "scripts": {
    "clean": "rm -rf dist/*",
    "build": "npm run clean && tsc && cross-env NODE_ENV=production webpack",
    "build:dev": "npm run clean && webpack --env environment=development",
    "build:watch": "npm run clean && webpack --env environment=development --watch",
